#include<iostream>
using namespace std;

int main(){
    int arr[40];
    int size;
    cout<<"Enter size of elements:"<<endl;
    cin>>size;

    cout<<"Enter elements of array:"<<endl;
    for(int i=0;i<size;i++){
        cin>>arr[i];
    }

    cout<<"Elements of array are:"<<endl;
    for(int i=0;i<size;i++){
        cout<<arr[i]<<endl;
    }

    return 0;

}
